const transformedData = {
    no: OBJ.no,
    title: OBJ.name,
    labels: OBJ.data.map(item => item.col),
    datasets: OBJ.data.map(item => {
      return {
        data: [item.val],
        label: item.col,
        total: parseInt(item.val)
      }
    })
  };
  
