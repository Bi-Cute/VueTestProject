const customCharts = {
    data: [
      { "col": "DUCK", "text1": "1", "text2": "1", "text3": "0" },
      { "col": "BONDU", "text1": "0", "text2": "0", "text3": "1" }
    ]
  };
  
  const transformedData = {
    labels: ["text1", "text2", "text3"],
    datasets: customCharts.data.map(dataset => {
      const data = Object.values(dataset).slice(1);
      const total = data.reduce((sum, value) => sum + Number(value), 0);
      return {
        data,
        label: dataset.col,
        total
      };
    })
  };
  
  console.log(transformedData);



